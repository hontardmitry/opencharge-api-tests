{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["ID", "AddressInfo", "NumberOfPoints"],
    "properties": {
      "ID": { "type": "integer" },
      "AddressInfo": {
        "type": "object",
        "required": ["Latitude", "Longitude"],
        "properties": {
          "Latitude": { "type": "number" },
          "Longitude": { "type": "number" }
        }
      },
      "NumberOfPoints": { "type": ["integer", "null"] }
    }
  }
}